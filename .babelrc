{
  "plugins": [
    "@babel/plugin-syntax-typescript",
    [
      "@babel/plugin-syntax-decorators",
      {
        "legacy": true
      }
    ],
    "@babel/plugin-syntax-jsx",
    "react-hot-loader/babel",
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": false,
        "useESModules": true
      }
    ]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": 'usage',
        "corejs": 3
      }
    ],
    "@babel/preset-react"
  ]
}
